    <script>
        document.getElementById('weekFilter').addEventListener('change', function() {
            const week = this.value;
            const rows = document.querySelectorAll('tbody tr:not(.week-header)');
            rows.forEach(row => {
                if (week === 'all' || row.dataset.week === week) {
                    row.style.display = '';
                } else {
                    row.style.display = 'none';
                }
            });
        });

        document.getElementById('platformFilter').addEventListener('change', function() {
            const platform = this.value;
            const rows = document.querySelectorAll('tbody tr:not(.week-header)');
            rows.forEach(row => {
                if (platform === 'all' || row.dataset.platform === platform) {
                    row.style.display = '';
                } else {
                    row.style.display = 'none';
                }
            });
        });
    </script>
</body>
</html>

