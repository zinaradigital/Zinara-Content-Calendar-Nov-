    <script>
        function toggleContent(btn) {
            const content = btn.nextElementSibling;
            content.classList.toggle('show');
            btn.classList.toggle('active');
            btn.textContent = content.classList.contains('show') ? 'üì± Hide Post ‚ñ≤' : 'üì± Read Post ‚ñº';
        }

        function toggleVideo(btn) {
            const content = btn.nextElementSibling;
            content.classList.toggle('show');
            btn.textContent = content.classList.contains('show') ? 'üìπ Hide ‚ñ≤' : 'üìπ Reel ‚ñº';
        }

        document.getElementById('weekFilter').addEventListener('change', function() {
            const week = this.value;
            document.querySelectorAll('tbody tr:not(.week-header)').forEach(row => {
                row.style.display = (week === 'all' || row.dataset.week === week) ? '' : 'none';
            });
        });

        document.getElementById('platformFilter').addEventListener('change', function() {
            const platform = this.value;
            document.querySelectorAll('tbody tr:not(.week-header)').forEach(row => {
                row.style.display = (platform === 'all' || row.dataset.platform === platform) ? '' : 'none';
            });
        });
    </script>
